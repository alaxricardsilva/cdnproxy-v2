# Configurações de otimização para npm
prefer-online=true
progress=false
audit=false
fund=false
save-exact=true
engine-strict=true

# Cache e performance
fetch-retries=3
fetch-retry-factor=2
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

# Reduzir logs durante build
loglevel=warn

# Configuração para dependências opcionais
omit=optional